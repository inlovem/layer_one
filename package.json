    "@fastify/type-provider-typebox": "^4.0.0",
    "@huggingface/transformers": "^3.1.0",
    "@ngneat/falso": "^2.27.0",
    "@sinclair/typebox": "^0.32.14",
    "@types/body-parser": "^1.19.5",
    "@types/crypto-js": "^4.2.2",
    "@types/node-cron": "^3.0.11",
    "@types/node-schedule": "^2.1.7",
    "@types/uuid": "^9.0.8",
    "axios": "^1.6.7",
    "body-parser": "^1.20.3",
    "crypto-js": "^4.2.0",
