      "import": "./dist/index.mjs"
    },
    "./src": {
      "default": "./src/index.ts"
    }
    },
  "scripts": {
    "dev": "NODE_OPTIONS='--loader ts-node/esm' nodemon src/index.ts",
    "build": "tsc",
    "start": "node ./dist/index.js"
  },
  "dependencies": {
