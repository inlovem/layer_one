{
  "name": "layer_one",
  "version": "0.1.1",
  "description": "Layer One API for use in GHL connections, hooks, and API calls for GHL APi V2",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "yarn": {
    "ignore-engines": true
  },
